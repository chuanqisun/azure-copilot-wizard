@top File { Statement+ }

Statement {
  Path Space Text*
}

Path {
  (Divider Segment)+
}

Text {
  Space | Number | Char | Symbol
}

@tokens {
  Space { @whitespace+ }
  Divider { "/" }
  Segment { $[A-Za-z_]+ }
  Number { $[0-9]+ }
  Char { @asciiLetter+ }
  Symbol { ![0-9A-Za-z_/]+ }
  @precedence { Space, Symbol }
}

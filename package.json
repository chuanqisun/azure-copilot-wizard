    "build:impromptu-web": "npm run build -w apps/impromptu-web",
    "dev:symphony-plugin": "npm run dev -w apps/symphony-plugin",
    "dev:symphony-lab": "npm run dev -w apps/symphony-lab",
    "dev:symphony-web": "npm run dev -w apps/symphony-web",
    "build:symphony-plugin": "npm run build -w apps/symphony-plugin",
    "build:symphony-lab": "npm run build -w apps/symphony-lab",
    "build:symphony-web": "npm run build -w apps/symphony-web"
  },
  "repository": {
    "type": "git",
